{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-zip": "*",
        "doctrine/dbal": "^2.12",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.5",
        "laravel/fortify": "^1.7",
        "laravel/framework": "^8.0",
        "laravel/pint": "^1.5",
        "laravel/sanctum": "^2.9",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "livewire/livewire": "^2.3",
        "maatwebsite/excel": "^3.1",
        "masterro/laravel-xss-filter": "^1.3",
        "milon/barcode": "^8.0",
        "nwidart/laravel-modules": "^6.2",
        "prewk/xml-streamer": "dev-master",
        "rap2hpoutre/fast-excel": "^5.0",
        "rap2hpoutre/laravel-log-viewer": "^1.6",
        "sentry/sentry-laravel": "^3.6.1",
        "spatie/enum": "^3.9",
        "spatie/laravel-activitylog": "^3.16",
        "spatie/laravel-medialibrary": "^9.0.0",
        "spatie/laravel-permission": "^3.9",
        "spatie/laravel-ray": "^1.32",
        "symfony/process": "^5.0",
        "tecnickcom/tcpdf": "^6.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^2.12",
        "facade/ignition": "^2.3",
        "fzaninotto/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/telescope": "^4.4",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0",
        "reliese/laravel": "^1.1",
        "prewk/xml-streamer": "dev-master"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "Alomgyar\\Affiliates\\": "packages/alomgyar/affiliates/",
            "Alomgyar\\Consumption_reports\\": "packages/alomgyar/consumption_reports/",
            "Alomgyar\\Carousels\\": "packages/alomgyar/carousels/",
            "Alomgyar\\Synchronizations\\": "packages/alomgyar/synchronizations/",
            "Alomgyar\\Suppliers\\": "packages/alomgyar/suppliers/",
            "Alomgyar\\Product_movements\\": "packages/alomgyar/product_movements/",
            "Alomgyar\\Methods\\": "packages/alomgyar/methods/",
            "Alomgyar\\Settings\\": "packages/alomgyar/settings/",
            "Alomgyar\\Warehouses\\": "packages/alomgyar/warehouses/",
            "Alomgyar\\Writers\\": "packages/alomgyar/writers/",
            "Alomgyar\\Legal_owners\\": "packages/alomgyar/legal_owners/",
            "Alomgyar\\Orders\\": "packages/alomgyar/orders/",
            "Alomgyar\\Carts\\": "packages/alomgyar/carts/",
            "Alomgyar\\Shops\\": "packages/alomgyar/shops/",
            "Alomgyar\\Templates\\": "packages/alomgyar/templates/",
            "Alomgyar\\Pages\\": "packages/alomgyar/pages/",
            "Alomgyar\\Coupons\\": "packages/alomgyar/coupons/",
            "Alomgyar\\Comments\\": "packages/alomgyar/comments/",
            "Alomgyar\\Countries\\": "packages/alomgyar/countries/",
            "Alomgyar\\Managment_templates\\": "packages/alomgyar/managment_templates/",
            "Alomgyar\\Promotions\\": "packages/alomgyar/promotions/",
            "Alomgyar\\Publishers\\": "packages/alomgyar/publishers/",
            "Alomgyar\\Posts\\": "packages/alomgyar/posts/",
            "Alomgyar\\Products\\": "packages/alomgyar/products/",
            "Alomgyar\\Authors\\": "packages/alomgyar/authors/",
            "Alomgyar\\Subcategories\\": "packages/alomgyar/subcategories/",
            "Alomgyar\\Categories\\": "packages/alomgyar/categories/",
            "Alomgyar\\Customers\\": "packages/alomgyar/customers/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Skvadcom\\Items\\": "packages/skvadcom/items/src/",
            "Skvadcom\\Logs\\": "packages/skvadcom/logs/",
            "Skvadcom\\General\\": "packages/skvadcom/general/src/",
            "Skvadcom\\Permissions\\": "packages/skvadcom/permissions/",
            "Alomgyar\\Recommenders\\": "packages/alomgyar/recommenders/",
            "Alomgyar\\Banners\\": "packages/alomgyar/banners/",
            "Alomgyar\\RankedProducts\\": "packages/alomgyar/ranked_products/",
            "Alomgyar\\Social\\": "packages/alomgyar/social",
            "Alomgyar\\PickUpPoints\\": "packages/alomgyar/pick_up_points",
            "Alomgyar\\PackagePoints\\": "packages/alomgyar/package_points",
            "Alomgyar\\Statistics\\": "packages/alomgyar/statistics",
            "Alomgyar\\InventoryExport\\": "packages/alomgyar/inventory_export",
            "Alomgyar\\BookRecommendation\\": "packages/alomgyar/book_recommendation/"
        },
        "files": [
            "packages/alomgyar/settings/support/helper.php",
            "packages/alomgyar/templates/support/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@php artisan package:discover --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
